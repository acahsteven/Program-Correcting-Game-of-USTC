            "#include<stdio.h>",
            "#include<string.h>",
            "",
            "#define MAXN 100000",
            "",
            "int len;",
            "char str1[MAXN], str2[MAXN], str3[MAXN];",
            "",
            "int main()",
            "{",
            "    scanf(\"%d %*[^%]%%%[^%]\", &len, str1);",
            "    sscanf(str1, \"%[^0-9]\", str2);",
            "    sscanf(str1, \"%*[^0-9]%*c%s\", str3);",
            "    printf(\"%*s\", len, strcat(str2, str3));",
            "    return 0;",
            "}"

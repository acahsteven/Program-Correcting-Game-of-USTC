            "int main()",
            "{",
            "    scanf(\"%d\", &out_len);",
            "    while(scanf(\"%c\", &ch)) {",
            "        if(ch == '%') {",
            "            if(!flag1)",
            "                flag1 = 1;",
            "            else",
            "                return;",
            "        } else {",
            "            if(!flag1)",
            "                continue;",
            "            else if(isdigit(ch)) {",
            "                if(!flag2)",
            "                    flag2 = 1;",
            "                else",
            "                    str[str_len++] = ch;",
            "            } else",
            "                str[str_len++] = ch;",
            "        }",
            "    }",
            "    printf(\"%*s\", out_len, str);",
            "    return 0;",
            "}"

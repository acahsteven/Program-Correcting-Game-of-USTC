			"#include<stdio.h>",
			"#include<string.h>",
			"char str[1001];",
			"int isop(char c){",
			"    switch(c){",
			"    case '+':",
			"        return 1;",
			"    case '-':",
			"        return 2;",
			"    case '*':",
			"        return 3;",
			"    case '/':",
			"        return 4;",
			"    default:",
			"        return 0;",
			"    }",
			"}",
			"int toInt(char* l,char* r){",
			"    int num=0;",
			"    for(char* p=l;p<=r;p++){",
			"        num=num*10+*p-'0';",
			"    }",
			"    return num;",
			"}",
			"int recursion(char*l,char*r){",
			"    if(l>r)return 0;",
			"    char* p=r;",
			"    int hidden=0;",
			"    while(p>=l){",
			"        if(*p=='(')hidden+=1;",
			"        if(*p==')')hidden-=1;",
			"        if(!hidden&&isop(*p)>0&&isop(*p)<=2)break;",
			"        p--;",
			"    }",
			"    if(p>=l){",
			"        if(isop(*p)==1)return recursion(l,p-1)+recursion(p+1,r);",
			"        else return recursion(l,p-1)-recursion(p+1,r);",
			"    }",
			"    else{",
			"        p=r;",
			"        int num_flag=1;",
			"        while(p>=l){",
			"            if(*p=='(')hidden+=1;",
			"            if(*p==')')hidden-=1;",
			"            if(isop(*p))num_flag=0;",
			"            if(!hidden&&isop(*p)>2)break;",
			"            p--;",
			"        }",
			"        if(p<l){",
			"            if(*l=='(')l++,r--;",
			"            if(num_flag)return toInt(l,r);",
			"            else return recursion(l,r);",
			"        }",
			"        else{",
			"            if(isop(*p)==3)return recursion(l,p-1)*recursion(p+1,r);",
			"            else return recursion(l,p-1)/recursion(p+1,r);",
			"        }",
			"    }",
			"}",
			"int main(){",
			"    int ans;",
			"    fgets(str,1000,stdin);",
			"    if(*(str+strlen(str)-1)=='\n')*(str+strlen(str)-1)='\\0';",
			"    ans=recursion(str,str+strlen(str)-1);",
			"    printf(\"%d\n\",ans);",
			"    return 0;",
			"}"

			"#include <stdio.h>",
			"#include <stdlib.h>",
			"#include <string.h>",
			"#include <ctype.h>",
			"",
			"typedef struct Node {",
			"    char op;            ",
			"    int value;          ",
			"    struct Node *left;",
			"    struct Node *right;",
			"} Node;",
			"",
			"Node* createValueNode(int val) {",
			"    Node* node = (Node*)malloc(sizeof(Node));",
			"    node->op = '\\0';",
			"    node->value = val;",
			"    node->left = node->right = NULL;",
			"    return node;",
			"}",
			"",
			"Node* createOpNode(char op,Node* l,Node* r){",
			"    Node* node = (Node*)malloc(sizeof(Node));",
			"    node->op = op;",
			"    node->left = l;",
			"    node->right = r;",
			"    return node;",
			"}",
			"",
			"Node* nodeStack[1000];",
			"int nodeTop = -1;",
			"char opStack[1000];",
			"int opTop = -1;",
			"",
			"void pushNode(Node* n){nodeStack[++nodeTop]=n;}",
			"Node* popNode() { return nodeStack[nodeTop--];}",
			"void pushOp(char op) { opStack[++opTop] = op;}",
			"char popOp() { return opStack[opTop--];}",
			"char peekOp(){return(opTop>=0)?opStack[opTop]:'\\0';}",
			"",
			"int priority(char op) {",
			"    if (op == '+' || op == '-') return 2;",
			"    if (op == '*' || op == '/') return 1;",
			"    return 0;",
			"}",
			"",
			"void buildSubTree() {",
			"    char op = popOp();",
			"    Node* node1 = popNode();",
			"    Node* node2 = popNode();",
			"    pushNode(createOpNode(op, node1, node2));",
			"}",
			"",
			"Node* buildExpressionTree(char* exp) {",
			"    for (int i = 0; exp[i] != '\\0'; i++) {",
			"        if (isspace(exp[i])) continue;",
			"        if (isdigit(exp[i])) {",
			"            int val = 0;",
			"            while (isdigit(exp[i])) {",
			"                val = val * 10 + (exp[i] - '0');",
			"                i++;",
			"            }",
			"            i--;",
			"            pushNode(createValueNode(val));",
			"        }",
			"        else if (exp[i] == '(') {",
			"            pushOp('(');",
			"        }",
			"        else if (exp[i] == ')') {",
			"            while (peekOp() != '(') {",
			"                buildSubTree();",
			"            }",
			"            popOp();",
			"        }",
			"        else {",
			"            while (opTop >= 0) {",
			"                int p1=priority(peekOp());",
			"                int p2=priority(exp[i]);",
			"                if(p1<p2)break;",
			"                buildSubTree();",
			"            }",
			"            pushOp(exp[i]);",
			"        }",
			"    }",
			"    while (opTop >= 0) {",
			"        buildSubTree();",
			"    }",
			"    return popNode();",
			"}",
			"",
			"int evaluate(Node* root) {",
			"    if (!root) return 0;",
			"    if (root->op == '\\0') return root->value;",
			"    int leftVal = evaluate(root->left);",
			"    int rightVal = evaluate(root->right);",
			"    switch (root->op) {",
			"        case '+': return leftVal + rightVal;",
			"        case '-': return leftVal - rightVal;",
			"        case '*': return leftVal * rightVal;",
			"        case '/': return leftVal / rightVal;",
			"    }",
			"    return 0;",
			"}",
			"",
			"void freeTree(Node* root) {",
			"    if (!root) return;",
			"    freeTree(root->left);",
			"    freeTree(root->right);",
			"    free(root);",
			"}",
			"",
			"int main() {",
			"    char exp[1000];",
			"    fgets(exp, 1000, stdin);",
			"    Node* root = buildExpressionTree(exp);",
			"    printf(\"%d\\n\", evaluate(root));",
			"    freeTree(root);",
			"    return 0;",
			"}"

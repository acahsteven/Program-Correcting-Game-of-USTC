			"#include<stdio.h>",
			"#include<string.h>",
			"char str[1001];",
			"int isop(char c){",
			"    switch(c){",
			"    case '+':",
			"        return 1;",
			"    case '-':",
			"        return 2;",
			"    case '*':",
			"        return 3;",
			"    case '/':",
			"        return 4;",
			"    default:",
			"        return 0;",
			"    }",
			"}",
			"int toInt(char* l,char* r){",
			"    int num=0;",
			"    for(char* p=l;p<=r;p++){",
			"        num=num*10+*p;",
			"    }",
			"    return num;",
			"}",
			"int recursion(char*l,char*r){",
			"    if(l>r)return 0;",
			"    char* p=l;",
			"    int hidden=0;",
			"    do{",
			"        if(*p=='(')hidden+=1;",
			"        if(*p==')')hidden-=1;",
			"        if(!hidden&&isop(*p)>0&&isop(*p)<=2)break;",
			"    }while((p++)>=r);",
			"    if(p<=r){",
			"        int leftrand=recursion(l,p-1);",
			"        int rightrand=recursion(p+1,r);",
			"        if(isop(*p)==1)return leftrand+rightrand;",
			"        else return leftrand-rightrand;",
			"    }",
			"    else{",
			"        p=r;",
			"        int num_flag=1;",
			"        do{",
			"            if(*p=='(')hidden+=1;",
			"            if(*p==')')hidden-=1;",
			"            if(isop(*p))num_flag=0;",
			"            if(!hidden&&isop(*p)>2)break;",
			"        }while((p--)>=l)",
			"        if(p<l){",
			"            if(*l=='(')l++,r--;",
			"            if(num_flag)return toInt(l,r);",
			"            else return recursion(l,r);",
			"        }",
			"        else{",
			"            int leftrand=recursion(l,p-1);",
			"            int rightrand=recursion(p+1,r);",
			"            if(isop(*p)==3)return leftrand*rightrand;",
			"            else return leftrand/rightrand;",
			"        }",
			"    }",
			"}",
			"int main(){",
			"    int ans;",
			"    fgets(str,1000,stdin);",
			"    if(*(str+strlen(str)-1)=='\\n'){",
			"        *(str+strlen(str)-1)='\\0';",
			"    }",
			"    ans=recursion(str,str+strlen(str)-1);",
			"    printf(\"%d\\n\",ans);",
			"    return 0;",
			"}"

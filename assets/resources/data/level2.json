			"#include<stdio.h>",
			"int main(){",
			"    const int pi=3.1415926;",
			"    int n;",
			"    scanf(\"%d\",&n);",
			"    int cnt=0,factorial=1;",
			"    double s=0,pow=0;",
			"    while(pow/factorial>1e-12){",
			"        if(cnt%4==1)s+=pow/factorial;",
			"        else if(cnt%4==3)s-=pow/factorial;",
			"        factorial*=++cnt;",
			"        pow*=2*pi/n;",
			"    }",
			"    printf(\"%.4llf\",s*0.5*n);",
			"    return 0;",
			"}"
        ]
